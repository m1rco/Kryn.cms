.kwindow-win {
    -moz-border-radius: 7px;
    position: absolute;
    left: 0px;
    right: 0px;
    top: 0px;
    bottom: 0px;
    border-top: 0px;
}

.kwindow-border {
    -moz-border-radius: 7px;
    
    -moz-border-radius-bottomleft: 0px;
    -moz-border-radius-bottomright: 0px;
    position: absolute;
    width: 400px;
    height: 300px;
    left: 50px;
    top: 0px;
    /*background-color: silver;*/
    border: 1px solid #777;
    
    
    background-color: #aaa;
    /*background-image: url(../images/win-content-bg.png);*/
    
    /*
    -moz-box-shadow: 0 0 2em gray;
    -webkit-box-shadow: 0 0 2em gray;
    box-shadow: 0 0 2em gray; */
}

.ka-kwindow-overlay {
    -moz-border-radius: 7px;
}

.kwindow-shadow-bottom {
    position: absolute;
    bottom: -42px;
    background-image: url(../images/kwindow-shadow-bottom.png);
    background-repeat: repeat-x;
    left: 30px;
    height: 41px;
    right: 30px;
}

.kwindow-shadow-left {
    position: absolute;
    bottom: 25px;
    left: -31px;
    width: 31px;
    background-image: url(../images/kwindow-shadow-left.png);
    background-repeat: repeat-y;
    top: 70px;
}

.kwindow-shadow-right {
    position: absolute;
    bottom: 25px;
    right: -31px;
    width: 31px;
    background-image: url(../images/kwindow-shadow-right.png);
    background-repeat: repeat-y;
    top: 70px;
}

.kwindow-shadow-top-right,
.kwindow-shadow-top-left {
    position: absolute;
    background-image: url(../images/kwindow-shadow-top-lr.png);
    background-repeat: no-repeat;
    top: 0px;
    width: 31px;
    height: 70px;
}

.kwindow-shadow-top-left {
    left: -31px;
}

.kwindow-shadow-top-right {
    right: -31px;
    background-position: -35px 0px;
}

.kwindow-shadow-bottom-left,
.kwindow-shadow-bottom-right {
    position: absolute;
    bottom: -42px;
    background-image: url(../images/kwindow-shadow-bottom-lr.png);
    background-repeat: no-repeat;
    width: 70px;
    height: 67px;
}

.kwindow-shadow-bottom-left {
    left: -40px;
}

.kwindow-shadow-bottom-right {
    right: -40px; 
    background-position: -119px 0px;
}

.kwindow-win-trans {
    position: absolute;
    left: 0px;
    right: 0px;
    top: 0px;
    bottom: 0px;
    background-color: #aaa;
    -moz-border-radius: 5px;
}

.kwindow-win-printer {
    position: absolute;
    left: 5px;
    top: 2px;
    cursor: pointer;
}

.kwindow-win-title,
.kwindow-win-title-inaktive {
    min-height: 22px;
    line-height: 20px;
    vertical-align: middle;
    font-weight: bold;
    text-indent: 5px;
    background-color: #d4d0c8;
    text-align: center;
    color: #111;
    -moz-border-radius-topleft: 7px;
    -moz-border-radius-topright: 7px;
    background-image: url(../images/win-top-bg.png);
    background-repeat: repeat-x;
    position: relative;
    cursor: default;
    border-bottom: 1px solid #555;
    border-right: 1px solid #aaa;
    border-left: 1px solid #aaa;
    border-top: 1px solid #ccc;
    text-shadow: 0 1px 0 #ccc;
}

.kwindow-win-titleGroups {
    overflow: hidden;
    height: 30px;
}

.kwindow-win-title-inaktive {
    color: #555;
}

.kwindow-win-content {
    position: absolute;
    left: 0px;
    right: 0px;
    top: 24px;
    bottom: 0px;
    background-color: #eee;
    overflow: auto;
}

.kwindow-win-bottom {
    position: absolute;
    left: 0px;
    right: 0px;
    display: none;
    height: 18px;
    bottom: 0px;
    /*background-color: #d4d0c8;*/
    background-image: url(../images/win-bottom-resize.png);
    background-repeat: no-repeat;
    background-position: right bottom;
    border-top: 1px solid #777;
    -moz-border-radius-bottomleft: 7px;
    -moz-border-radius-bottomright: 7px;
    padding-left: 6px;
    color: #444;
    line-height: 18px;
}

.kwindow-win-bottom-maximized {
    background-image: none;
}

.kwindow-iframe {
    width: 100%;
    height: 100%;
    border: 0px;
    padding: 0px;
    margin: 0px;
}

.kwindow-win-titleBar {
    position: absolute;
    top: 3px;
    right: 4px;
    height: 16px;
}

.kwindow-win-titleBarIcon {
    margin-left: 4px;
    width: 16px;
    height: 16px;
    background-repeat: no-repeat;
}

.kwindow-win-titleBarIcon-close {
    background-image: url(../images/win-top-bar-close.png);
    background-position: left top;
    float: right;
}

.kwindow-win-titleBarIcon-close:hover {
    background-position: left -16px;
}

.kwindow-win-titleBarIcon-close:active {
    background-position: left -32px;
}

.ka-kwindow-prompt {
    position: absolute;
    left: 20px;
    top: 20px;
    width: 230px;
    background-color: #eee;
    border: 6px solid #bbb;
    -moz-border-radius: 5px;
    padding: 10px;
    padding-bottom: 40px;
}


.ka-kwindow-prompt input.text {
    width: 220px;
}

.ka-kwindow-prompt-text {
    padding-bottom: 5px;
    padding-top: 4px;
}

.ka-kwindow-prompt-bottom {
    position: absolute;
    text-align: right;
    left: 0px;
    right: 10px;
    bottom: 8px;
    height: 25px;
}

.kwindow-win-content h3 {
    margin: 0px;
    cursor: default;
    font-weight: normal;
    padding-left: 1px;
    color: #555;
    border-bottom: 1px solid silver;
    height: 19px;
    line-height: 19px;
    font-size: 11px;
    background-color: #ddd;
    padding: 0px 3px;
}

.ka-kwindow-content-withBottomBar {
    position: absolute;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 31px;
    overflow: auto;
}
