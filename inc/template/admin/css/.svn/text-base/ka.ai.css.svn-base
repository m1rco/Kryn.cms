html, body {
    font-family: Arial,sans;
    font-size: 11px;
    margin: 0px;
    padding: 0px;
    overflow: hidden;
}

body {
    height: 100%;
}

img {
    border: 0px;
}

input, select, textarea {
    outline: 0;	
}

select {
    font-size: 11px;
    border: 1px solid silver;
    padding: 1px;
}

html {
    height: 100%;
}

.border {
    position: absolute;
    left: 0px;
    right: 0px;
    top: -10px;
    bottom: 0px;
}

/* main layout */
div.header {
    position: absolute;
    z-index: 2600000; /*2.6mill*/
    left: 47px;
    right: 45px;
    text-align: left;
    height: 39px;
    top: 16px;
    background-repeat: repeat-x;
    background-image: url(../images/menubar-bg.png);
}

div.header-left {
    position: absolute;
    left: -13px;
    top: 0px;
    height: 39px;
    width: 13px;
    background-image: url(../images/menubar-left.png);
}

div.header-right {
    position: absolute;
    right: -13px;
    top: 0px;
    height: 39px;
    width: 13px;
    background-image: url(../images/menubar-right.png);
}

div.headRight .lastItem {
    -moz-border-radius-topright: 13px;
    -moz-border-radius-bottomright: 13px;
    -webkit-border-bottom-right-radius: 13px;
    -webkit-border-top-right-radius: 13px;
    border-bottom-right-radius: 13px;
    border-top-right-radius: 13px;
}

div.headRight img.arrowUp:hover {
    background-color: #399bc7;
}

div.mainlinks {
    position: absolute;
    top: 0px;
    left: -11px;
}

div.headRight a img,
div.mainlinks a img {
    position: absolute;
    top: 10px;
    left: 8px;
    width: 16px;
    height: 16px;
}

div.headRight > a,
div.headRight > a:link,
div.mainlinks > a,
div.mainlinks > a:link,
div.ka-mainlink-additionalmenubar {
    float: left;
    line-height: 32px;
    padding: 2px 19px 0px 34px;
    border-right: 1px solid #868588;
    font-weight: bold;
    color: white;
    text-decoration: none;
    position: relative;
    z-index: 16000;
    display: block;
    height: 32px;
    cursor: pointer;
    outline: 0;
}

div.ka-mainlink-additionalmenubar a,
div.ka-mainlink-additionalmenubar a:link {
	color: white;
	padding: 4px 7px;
    border-bottom: 1px solid #868588;
}

div.ka-mainlink-additionalmenubar-container {
	position: absolute;
	left: 0px;
	top: 0px;
    -moz-border-radius-bottomleft: 10px;
    -moz-border-radius-bottomright: 10px;
    -webkit-border-bottom-left-radius: 10px;
    -webkit-border-bottom-right-radius: 10px;
    border-bottom-left-radius: 10px;
    border-bottom-right-radius: 10px;
}

div.mainlinks a.bar-dock-logo img { 
    width: auto;
    height: auto;
}

div.mainlinks a.bar-dock-logo { 
    height: 32px;
    width: 30px;
    padding-left: 0px;
    padding-right: 0px;
}

div.bar-dock-logo-menu-style {
    position: absolute;
    z-index: 2550000;
    padding-top: 12px;
    width: 170px;
    min-height: 50px;
    -moz-box-shadow: 0 0 2em #656465;
    -webkit-box-shadow: 0 0 2em #656465;
    box-shadow: 0 0 2em #656465;
    background-color: #656465;
}

div.bar-dock-logo-menu { 
    left: 36px;
    top: 40px;
    -moz-border-radius-bottomleft: 10px;
    -moz-border-radius-bottomright: 10px;
    -webkit-border-bottom-left-radius: 10px;
    -webkit-border-bottom-right-radius: 10px;
    border-bottom-left-radius: 10px;
    border-bottom-right-radius: 10px;
}

div.bar-dock-logo-menu a,
div.ka-mainlink-additionalmenubar-container a {
    display: block;
    padding: 6px 5px 6px 20px;
    margin: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    cursor: default;
    color: white;
    cursor: pointer;
    position: relative;
}


div.ka-mainlink-additionalmenubar-container a {
	position: relative;
}

div.ka-mainlink-additionalmenubar-container a img {
	position: absolute;
	left: 2px;
	top: 3px;
}

.bar-dock-logo-menu-submenu {
    position: absolute;
    right: 0px;
    top: -10px;
}

div.headRight > a:hover,
div.mainlinks > a:hover,
div.ka-mainlink-additionalmenubar-container a:hover {
    background-image: url(../images/menubar-active-item.png);
    background-repeat: repeat-x;
}


div.mainlinks a.first,
div.mainlinks a.first:link {
    -moz-border-radius-topleft: 13px;
    -moz-border-radius-bottomleft: 13px;
    -webkit-border-top-left-radius: 13px;
    -webkit-border-bottom-left-radius: 13px;
    border-top-left-radius: 13px;
    border-bottom-left-radius: 13px;
}

div.headRight > a,
div.headRight > a:link {
    border-right: 0px;
    border-left: 1px solid #868588;
    float: right;
}

/*
div.headRight > a.last,
div.headRight > a.last:link {
    -moz-border-radius-topright: 11px;
    -moz-border-radius-bottomright: 11px;
} */

/* module bar */

.ka-module-menu {
    position: absolute;
    left: 0px;
    top: 70px;
    bottom: 40px;
    min-width: 200px;
    background-image: url(../images/module-bar-bg.png);
    background-repeat: repeat-x;
    padding-top: 7px; 
    background-color: #656465;
    overflow: visible;
    -moz-border-radius-topright: 9px;
    -moz-border-radius-bottomright: 9px;
    -webkit-border-top-right-radius: 9px;
    -webkit-border-bottom-right-radius: 9px;
    border-top-right-radius: 9px;
    border-bottom-right-radius: 9px;
    z-index: 2500000; /*2.5mill*/
}

.ka-module-menu div.kwindow-win-buttonGroup {
    border-bottom: 1px solid #737274;
    border-right: 1px solid #6c6b6d;
    border-left: 1px solid #6c6b6d;
    border-top: 1px solid #5e5d5e;
    margin-left: 4px;
    margin-right: 5px;
}

.ka-module-items {
    position: absolute;
    top: 35px;
    left: 0px;
    right: 40px;
    overflow: hidden;
    bottom: 25px;
    padding-bottom: 26px;
    padding-left: 6px;
    padding-right: 6px;
    background-color: #656465;
    -moz-border-radius-bottomright: 9px;
    -webkit-border-bottom-right-radius: 9px;
    border-bottom-right-radius: 9px;
}

.ka-module-items-scroller-container {
    position: absolute;
    left: 0px;
    top: 35px;
    bottom: 25px;
    width: 12px;
}

.ka-module-items-scroller {
    position: absolute;
    top: 0px;
    height: 25px;
    width: 12px;
    -moz-border-radius: 5px;
    background-color: #444;
}


.ka-module-items a {
    display: block;
    padding: 6px 4px 6px 8px;
    color: white;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px;
    font-weight: bold;
    margin-left: 9px;
    cursor: pointer;
    border-bottom: 1px solid #6b6b6b;
}

.ka-module-items a:hover {
    color: white;	
}


.bar-dock-logo-menu a:hover,
.ka-module-items .ka-module-container a:hover {
	cursor: default;
	background-color: #617781;
}

.bar-dock-logo-menu a.ka-module-items-activated:hover,
.ka-module-items .ka-module-items-activated a:hover {
    background-color: #399bc7;
    cursor: pointer;
}

.ka-module-toggler {
    position: absolute;
    top: 0px;
    bottom: 0px;
    right: 0px;
    width: 40px;
    
    cursor: pointer;
    text-align: center;
    
    background-image: url(../images/module-bar-button.png);
    background-position: right top;
    background-repeat: no-repeat;
    
    -moz-border-radius-bottomright: 11px;
    -moz-border-radius-topright: 11px;
    -webkit-border-bottom-right-radius: 11px;
    -webkit-border-top-right-radius: 11px;
    border-bottom-right-radius: 11px;
    border-top-right-radius: 11px;
}

.ka-module-toggler:hover {
    background-color: #287ca4;
    background-image: url(../images/module-bar-button-active.png);
}

.ka-module-toggler img {
    margin-top: 45px;
    margin-left: 2px;
}

/* clipboard */

.ka-clipboard-menu,
.ka-updates-menu,
.ka-upload-menu {
    position: absolute;
    top: 20px;
    z-index: 2500002;
    right: 310px;
    width: 200px;
    text-align: left;
    line-height: 24px;
    height: 24px;
    padding: 0px 0px 0px 5px;
    color: white;
    background-image: url(../images/userInfo-bg.png);
    -moz-border-radius-bottomright: 7px;
    -moz-border-radius-bottomleft: 7px;
    -webkit-border-bottom-left-radius: 7px;
    -webkit-border-bottom-right-radius: 7px;
    border-bottom-left-radius: 7px;
    border-bottom-right-radius: 7px;
    overflow: hidden;
}

.ka-updates-menu { 
    left: 60px;
    right: auto;
    width: 100px;
    text-align: center;
    padding: 0px;
    cursor: pointer;
}

.ka-upload-menu {
    right: 530px;
    padding: 0px;
    background-image: none;
}

.ka-upload-menu-info {
    padding: 2px;
    -moz-border-radius: 7px;
    -webkit-border-radius: 7px;
    border-radius: 7px;
    border: 1px solid #aaa;
    background-color: #444;
    color:white;
    line-height: 19px;
    padding: 1px 0px 0px 5px;
}

.ka-upload-menu .item {
    position: relative;
    height: 20px;
    line-height: 20px;
    margin-bottom: 2px;
    cursor: pointer;
}

.ka-upload-menu .item .title {
    position: absolute;
    top: 1px;
    left: 3px;
    cursor: pointer;
    color: #f3f3f3;
}

.ka-upload-menu .item .pbar {
    position: absolute;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 20px;
    text-align: right;
    cursor: pointer;
}

.ka-upload-menu .item .pbarProc {
    position: absolute;
    top: 1px;
    right: 0px;
    height: 19px;
    padding-right: 3px;
    left: 0px;
    /*background-image: url(../images/userInfo-bg.png);*/
    background-color: #888;
    -moz-border-radius: 7px;
    -webkit-border-radius: 7px;
    border-radius: 7px;
    border: 1px solid #aaa;
    margin: auto;
    cursor: pointer;
}

.ka-upload-menu .item .pbarProgress {
    position: absolute;
    left: 0px;
    top: 1px;
    height: 20px;
    width: 1%;
    background-color: #eee;
    margin: auto;
    -moz-border-radius: 7px;
    -webkit-border-radius: 7px;
    border-radius: 7px;
    cursor: pointer;
}

/* userInfo */
.userInfo {
    position: absolute;
    top: 50px;
    right: 71px;
    background-image: url(../images/userInfo-bg.png);
    -moz-border-radius-bottomright: 7px;
    -moz-border-radius-bottomleft: 7px;
    -webkit-border-bottom-right-radius: 7px;
    -webkit-border-bottom-left-radius: 7px;
    border-bottom-right-radius: 7px;
    border-bottom-left-radius: 7px;
    width: 190px;
    padding-right: 17px;
    color: white;
    text-align: right;
    line-height: 24px;
    z-index: 2500001;
}


/* subnavi */

div.ka-subnavi {
    position: absolute;
    min-width: 150px;
    z-index: 16000;
    padding: 5px;
    -moz-border-radius-topright: 10px;
    -moz-border-radius-bottomright: 10px;
    -webkit-border-top-right-radius: 10px;
    -webkit-border-bottom-right-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    /*border-bottom: 1px solid silver;*/
    background-color: #656466;
    
    
    -moz-box-shadow: 0 0 2em #656465;
    -webkit-box-shadow: 0 0 2em #656465;
    box-shadow: 0 0 2em #656465;
    
}

img.ka-subnavi-top-left,
img.ka-subnavi-bottom-left {
    position: absolute;
}

img.ka-subnavi-top-left {
    left: 0px;
    top: -10px;
}

img.ka-subnavi-bottom-left {
    left: 0px;
    bottom: -10px;
}

a.ka-subnavi, 
a.ka-subnavi:link { 
    position: relative;
    float: none;
    z-index: 16000;
    color: white;
    padding: 6px 5px 6px 10px;
    display: block;
    cursor: pointer;
}

a.ka-subnavi:hover {
    background-color: #399bc7;
}


div.ka-subnavi-npa a.ka-subnavi,
div.ka-subnavi-npa a.ka-subnavi:link {
    font-size: 11px;
    padding: 3px 5px 3px 25px;
    font-weight: normal;
    color: #ddd;
}

a.ka-subnavi-main,
a.ka-subnavi-main:link {
    color: #ddd;	
}


/* windlow list */
